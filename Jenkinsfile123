pipeline{
    agent any
    stages{
        stage('git checkout'){
            steps{
                checkout scmGit(branches: [[name: '*/featurea']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/mohandevops1869/myjavaapp.git']])
            }
        }
        stage ('Build') {
            git url: 'https://github.com/mohandevops1869/myjavaapp.git'
            withMaven {
                  sh "mvn clean verify"
                } // withMaven will discover the generated Maven artifacts, JUnit Surefire & FailSafe reports and FindBugs reports
        }
    }
}